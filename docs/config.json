{
  "auto_login": false,
  "allow_logout": false,
  "domain_placeholder": "wikisuite.chat",  
  "authentication": "login",
  "auto_away": 300,
  "notification_icon": "./css/images/logo/conversejs-filled.svg",
  "view_mode": "fullscreen",
  "muc_show_join_leave_status": true,
  "play_sounds": true,
  "notify_all_room_messages": [],
  "muc_mention_autocomplete_min_chars": 0,
  "assets_path": "./dist/",
  "clear_messages_on_reconnection": true,

  "default_domain": "wikisuite.chat",
  "domain_placeholder": "wikisuite.chat",   
  "bosh_service_url": "https://wikisuite.chat:7443/http-bind/",
  "websocket_url": "wss://wikisuite.chat:7443/ws/",  

  "auto_reconnect": true,
  "roster_groups": true,
  "singleton": false,
  "debug": false,
  "message_archiving": "always",
  "auto_focus": true,
  "i18n": "en",
  "show_message_load_animation": false,
  "allow_message_corrections": "all",
  "whitelisted_plugins": [
    "content", 
    "canned", 
    "screencast", 
    "vmsg", 
    "directory", 
    "search", 
    "jitsimeet", 
    "audioconf", 
    "fastpath",
    "browsertab",     
    "location",    
    "settings",     
    "pade"
  ],
  "message_carbons": true,
  "muc_fetch_members": false,
  "blacklisted_plugins": [
    "converse-minimize",
    "converse-dragresize"
  ], 
  "show_client_info": false,
  "message_limit": 0,
  "sounds_path": "./dist/sounds/",
  "enable_smacks": false,
  "auto_join_rooms": ["wikisuite@conference.wikisuite.chat"]
}